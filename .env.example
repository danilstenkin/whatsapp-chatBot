# Example .env file for WhatsApp GPT Bot

# PostgreSQL Database
DB_USER=myuser
DB_PASS=mypassword
DB_NAME=whatsapp
DB_HOST=db
DB_PORT=5432

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Twilio WhatsApp API
TWILIO_ACCOUNT_SID=ACXXXXXXXXXXXXXXXXXXXXXXXXXXXX
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+1XXXXXXXXXX

# OpenRouter API (GPT Provider)
OPENROUTER_API_KEY=sk-or-your-openrouter-key

# DeepSeek AI API (optional)
DEEPSEEK_API_KEY=sk-your-deepseek-key

# Encryption Key (for encrypting sensitive data)
ENCRYPTION_KEY=your_base64_encoded_encryption_key
